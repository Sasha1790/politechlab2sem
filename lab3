#include <stdio.h>
#include <math.h>

int main()
{
    double x,y,fin;
    
    printf("Введіть масу в кілограмах (m): ");
    scanf("%lf", &x);
    
    printf("Введіть швидкість тіла (v): ");
    scanf("%lf", &y);
    
    fin = 0.5 * x * pow(y, 2);
    
    printf("Кінетична енергія тіла: %lf Дж\n ",fin);
    
    return 0;
    
}
